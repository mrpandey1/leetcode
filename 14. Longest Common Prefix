class Solution:
    def longestCommonPrefix(self, a: List[str]) -> str:
        '''
        temp=sorted(a,key=len)
        res=''
        ans=''
        flag=True
        if (len(temp)==0): 
            return "" 
        if (len(temp)==1): 
            return a[0]
        for i in temp[0]:
            res+=i
            for j in a:
                if j.startswith(res):
                    ans=res
                else:
                    flag=False
            if flag==False:
                return res[0:-1]
        '''
        length=len(a)  
        a.sort()  
        if (length==0): 
            return "" 
        if (length==1): 
            return a[0]
        smallest=min(len(a[0]),len(a[length-1]))
        print(smallest)
        i = 0
        while i <smallest and a[0][i]==a[length-1][i]:
            i+=1
        ans=a[0][0:i] 
        return ans
        
