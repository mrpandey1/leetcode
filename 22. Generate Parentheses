class Solution:
    def generateParenthesis(self, n):
        res = []
        def dfs (l, r, path, res):
            if r < l or l == -1 or r == -1:
                return
            if l == 0 and r == 0:
                res.append(path[:])
            else:
                dfs(l-1, r, path + "(", res)
                dfs(l, r-1, path + ")", res)
        dfs(n, n, "", res)
        return (res)
