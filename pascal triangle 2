class Solution:
    def getRow(self, n: int) -> List[int]:
        a=[]
        if n==0:
            return [1]
        b=n
        for i in range(n+1):
            a.append([])
            a[i].append(1)
            for j in range(1,i):
                a[i].append(a[i-1][j-1]+a[i-1][j])
            if(n!=0):
                a[i].append(1)
        del a[0][1]
        return a[n]
